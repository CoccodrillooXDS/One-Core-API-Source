!IF "$(DDK_TARGET_OS)"=="Win2K"
!   message This sample is not intended to target the Windows 2000 platform.
!ELSEIF "$(DDK_TARGET_OS)"=="WinXP"
!   message This sample is not intended to target the Windows XP platform.
!ELSEIF "$(DDK_TARGET_OS)"=="WinNET"
!   message This sample is not intended to target the Windows 2003 platform.
!ELSEIF "$(DDK_TARGET_OS)"=="WinLH"
!   message This sample is not intended to target the Windows Vista or Windows 2008 platform.
!ELSEIF "$(DDK_TARGET_OS)"=="Win7"
!   message This sample is not intended to target the Windows 7 platform.
!ENDIF

# this WDK sample requires Windows 8 or higher WDK environment
MINIMUM_NT_TARGET_VERSION=0x602

